{
  "name": "@ibgrav/usafacts-swapi",
  "packageManager": "pnpm@8.11.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel ",
    "dev.backend": "tsx watch --clear-screen=false -r dotenv/config backend/main.ts",
    "dev.frontend": "vite dev",
    "dev.storybook": "sb dev -p 62677 --no-open",
    "build.backend": "tsc -p ./tsconfig.build.json",
    "build.frontend": "vite build",
    "build.storybook": "sb build --webpack-stats-json",
    "test.types": "tsc --noEmit",
    "test.vitest": "vitest",
    "test.lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test.storybook": "tsx bin/test-storybook.ts",
    "preview.storybook": "sirv --host --quiet --port 6006 storybook-static"
  },
  "dependencies": {
    "sirv": "2.0.3"
  },
  "devDependencies": {
    "@nivo/bar": "0.84.0",
    "@storybook/addon-essentials": "7.6.3",
    "@storybook/addon-interactions": "7.6.3",
    "@storybook/cli": "7.6.3",
    "@storybook/react": "7.6.3",
    "@storybook/react-vite": "7.6.3",
    "@storybook/test": "7.6.3",
    "@storybook/test-runner": "0.16.0",
    "@types/node": "20.10.2",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react": "4.2.0",
    "dotenv": "16.3.1",
    "eslint": "8.55.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "light-my-request": "5.11.0",
    "msw": "^1",
    "msw-storybook-addon": "1.10.0",
    "prettier": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "sirv-cli": "2.0.2",
    "tsx": "4.6.2",
    "typescript": "~5.2",
    "vite": "^4",
    "vitest": "0.34.6",
    "wait-on": "7.2.0",
    "zx": "7.2.3"
  }
}
